function ordenarPorIdade(pessoas) {
    return pessoas.sort((a, b) => a.idade - b.idade);
}

const pessoas = [
    { nome: "Thiago", idade: 60 },
    { nome: "Pedro", idade: 25 },
    { nome: "Lucas", idade: 30 },
    { nome: "Joao", idade: 35 }
];

const pessoasOrdenadas = ordenarPorIdade(pessoas);
console.log(pessoasOrdenadas);
